{
  "name": "electron-window-state-async",
  "version": "1.0.1-0",
  "description": "Electron window state async store",
  "private": true,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "electron-mocha test.js",
    "build": "tsc -p .",
    "clean": "rimraf ./built",
    "rebuild": "npm run clean && npm run build",
    "doc": "rimraf docs && typedoc --theme minimal --out docs --mode file --exclude \"**/__test__//*\"  --excludeNotExported",
    "dist": "tsc -p tsconfig.prod.json",
    "redist": "rimraf ./dist && npm run dist"
  },
  "keywords": [
    "eLectron",
    "window",
    "state",
    "async",
    "store"
  ],
  "author": "D10221",
  "license": "MIT",
  "dependencies": {
    "debug": "^2.6.8",
    "electron-json-storage-async": "git+https://github.com/D10221/electron-json-storage-async.git",
    "fs-extra": "^3.0.1"
  },
  "devDependencies": {
    "@types/debug": "0.0.29",
    "@types/electron": "^1.4.38",
    "@types/fs-extra": "^3.0.2",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.22",
    "@types/shelljs": "^0.7.1",
    "electron": "^1.6.10",
    "electron-mocha": "^3.4.0",
    "shelljs": "^0.7.7"
  }
}
